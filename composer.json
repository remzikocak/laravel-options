{
    "name": "remzikocak/laravel-options",
    "description": "Options Package for Laravel 7",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Remzi Kocak",
            "email": "kocak0068@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "RKocak\\Options\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RKocak\\Options\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RKocak\\Options\\OptionsServiceProvider"
            ],
            "aliases": {
                "Options": "RKocak\\Options\\Facades\\Options"
            }
        }
    }
}
